{
    "autoDocstring.docstringFormat": "numpy",
    "bracket-pair-colorizer-2.showBracketsInGutter": true,
    "bracket-pair-colorizer-2.showBracketsInRuler": true,
    "breadcrumbs.enabled": true,
    "breadcrumbs.symbolSortOrder": "position",
    "cobiSymbolOutline.sortBy": "Position",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.fontFamily": "Hack, Consolas, 'Courier New', monospace",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.largeFileOptimizations": true,
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 160,
    "editor.minimap.showSlider": "always",
    "editor.renderIndentGuides": false,
    "editor.renderWhitespace": "none",
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.favorites.json": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.idea": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.sonarlint": true,
        "**/.svn": true,
        "**/.vscode": true,
        "**/codealike.json": true
    },
    "files.maxMemoryForLargeFilesMB": 8192,
    "git.autofetch": true,
    "markdown.preview.breaks": true,
    "material-icon-theme.folders.color": "#c0c0c0c2",
    "material-icon-theme.hidesExplorerArrows": false,
    "python.dataScience.exportWithOutputEnabled": true,
    "python.formatting.provider": "yapf",
    "python.jediEnabled": true,
    "python.linting.enabled": true,
    "python.linting.pylamaEnabled": true,
    "python.linting.pylintEnabled": true,
    "python.venvPath": "~/.virtualenvs",
    "scm.alwaysShowActions": true,
    "scm.alwaysShowProviders": true,
    "terminal.integrated.fontFamily": "Source Code Pro",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.shell.windows": "C:\\windows\\System32\\cmd.exe",
    "todo-tree.customHighlight": {
        "DEBUG": {
            "icon": "bug",
            "iconColour": "#c0c0c0f8"
        },
        "FIXME": {
            "icon": "alert",
            "iconColour": "#FBB034"
        },
        "HACK": {
            "icon": "tools",
            "iconColour": "#c0c0c0f8"
        },
        "NOTE": {
            "icon": "info",
            "iconColour": "#c0c0c0f8"
        },
        "REVIEW": {
            "icon": "search",
            "iconColour": "#c0c0c0f8"
        },
        "TODO": {
            "icon": "check",
            "iconColour": "#18AAFD",
            "type": "line"
        }
    },
    "todo-tree.defaultHighlight": {
        "icon": "repo",
        "iconColour": "#c0c0c0c2"
    },
    "todo-tree.statusBar": "top three",
    "todo-tree.tags": [
        "TODO",
        "FIXME",
        "HACK",
        "NOTE",
        "REVIEW",
        "DEBUG"
    ],
    "todohighlight.keywords": [{
            "backgroundColor": "#EF3E42",
            "color": "#F4F4F4",
            "text": "DEBUG:"
        },
        {
            "backgroundColor": "#c0c0c0f8",
            "color": "#000000",
            "text": "REVIEW:"
        },
        {
            "backgroundColor": "#AEE0E8",
            "color": "#000000",
            "text": "NOTE:"
        },
        {
            "backgroundColor": "#FBB034",
            "color": "#000000",
            "text": "FIXME:"
        },
        {
            "backgroundColor": "#439539",
            "color": "#F4F4F4",
            "text": "HACK:"
        },
        {
            "backgroundColor": "#18AAFD",
            "color": "#F4F4F4",
            "text": "TODO:"
        }
    ],
    "window.title": "${appName}${separator}${rootName}${separator}${dirty}${activeEditorShort}",
    "workbench.colorCustomizations": {
        "statusBar.background": "#19257175",
        "statusBar.foreground": "#c0c0c0f8"
    },
    "workbench.colorTheme": "One Dark+ (Sublime)",
    "workbench.iconTheme": "file-icons"
}
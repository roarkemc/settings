{
    "terminal.integrated.shell.windows": "C:\\Windows\\system32\\cmd.exe",
    "editor.minimap.enabled": true,
    "editor.wordWrap": "on",
    "python.autoComplete.preloadModules": [
        "statsmodels",
        "sqlalchemy",
        "numpy",
        "pandas",
        "matplotlib",
        "seaborn",
        "scipy",
        "sklearn"
    ],
    "autoDocstring.docstringFormat": "numpy",
    "atomKeymap.promptV3Features": true,
    "editor.formatOnPaste": true,
    "workbench.activityBar.visible": true,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "window.menuBarVisibility": "default",
    "TodoParser": {
        "exclude": [
            "cpp",
            "c"
        ],
        "include": [
            "py"
        ],
        "folderExclude": [
            "node_modules",
            ".vscode"
        ],
        "only": [
            "sub-folder/sub-sub-folder"
        ],
        "showInProblems": false,
        "markers": [
            "NOTE:",
            "REMINDER:"
        ],
        "autoDefaultMarkers": true
    },
    "editor.cursorStyle": "line-thin",
    "workbench.iconTheme": "file-icons",
    "material-icon-theme.folders.color": "#90a4ae",
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#7210aa",
        "list.activeSelectionForeground": "#616161",
        "list.inactiveSelectionForeground": "#616161",
        "list.highlightForeground": "#616161",
        "scrollbarSlider.activeBackground": "#616161",
        "editorSuggestWidget.highlightForeground": "#616161",
        "textLink.foreground": "#616161",
        "progressBar.background": "#C6FF00",
        "pickerGroup.foreground": "#616161",
        "tab.activeBorder": "#616161",
        "statusBar.background": "#6c0bad8f"
    },
    "workbench.sideBar.location": "left",
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "extensions.ignoreRecommendations": false,
    "workbench.colorTheme": "TwoDark"
}